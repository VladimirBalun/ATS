<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	   					   http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>

    <context:component-scan base-package="ru.tidstu.testingsystem.data.dao" />
    <context:component-scan base-package="ru.tidstu.testingsystem.data.service" />

    <bean id="basicDataDAO" class="ru.tidstu.testingsystem.data.dao.BasicDataDAOImpl"/>
    <bean id="groupsDAO" class="ru.tidstu.testingsystem.data.dao.GroupsDAOImpl"/>
    <bean id="resultsDAO" class="ru.tidstu.testingsystem.data.dao.ResultsDAOImpl"/>
    <bean id="usersDAO" class="ru.tidstu.testingsystem.data.dao.UsersDAOImpl"/>
    <bean id="questionsDAO" class="ru.tidstu.testingsystem.data.dao.QuestionsDAOImpl"/>
    <bean id="testDataDAO" class="ru.tidstu.testingsystem.data.dao.TestDataDAOImpl"/>

    <bean id="basicDataService" class="ru.tidstu.testingsystem.data.service.BasicDataServiceImpl"/>
    <bean id="groupsService" class="ru.tidstu.testingsystem.data.service.GroupsServiceImpl"/>
    <bean id="resultsService" class="ru.tidstu.testingsystem.data.service.ResultsServiceImpl"/>
    <bean id="usersService" class="ru.tidstu.testingsystem.data.service.UsersServiceImpl"/>
    <bean id="questionsService" class="ru.tidstu.testingsystem.data.service.QuestionsServiceImpl"/>
    <bean id="testDataService" class="ru.tidstu.testingsystem.data.service.TestDataServiceImpl"/>



    <bean id="olympiad" class="ru.tidstu.testingsystem.utils.PassingOlympiad"/>

</beans>